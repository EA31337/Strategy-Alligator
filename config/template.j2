/**
 * @file
 * Defines default strategy parameter values for the given timeframe.
 */

// Defines indicator's parameter values for the given pair symbol and timeframe.
struct Indi_Alligator_Params_{{ Timeframe }} : AlligatorParams {
  Indi_Alligator_Params_{{ Timeframe }}() : AlligatorParams(indi_alli_defaults, PERIOD_{{ Timeframe }}) {
    applied_price = (ENUM_APPLIED_PRICE){{ Alligator_Indi_Alligator_Applied_Price | default(0) }};
    jaw_period = {{ Alligator_Indi_Alligator_Period_Jaw | default(0) }};
    jaw_shift = {{ Alligator_Indi_Alligator_Shift_Jaw | default(0) }};
    lips_period = {{ Alligator_Indi_Alligator_Period_Lips | default(0) }};
    lips_shift = {{ Alligator_Indi_Alligator_Shift_Lips | default(0) }};
    ma_method = (ENUM_MA_METHOD){{ Alligator_Indi_Alligator_MA_Method | default(0) }};
    shift = {{ Alligator_Indi_Alligator_Shift | default(0) }};
    teeth_period = {{ Alligator_Indi_Alligator_Period_Teeth | default(0) }};
    teeth_shift = {{ Alligator_Indi_Alligator_Shift_Teeth | default(0) }};
  }
} indi_alli_{{ Timeframe | lower }};

// Defines strategy's parameter values for the given pair symbol and timeframe.
struct Stg_Alligator_Params_{{ Timeframe }} : StgParams {
  // Struct constructor.
  Stg_Alligator_Params_{{ Timeframe }}() : StgParams(stg_alli_defaults) {
    lot_size = {{ Alligator_LotSize | default(0) }};
    signal_open_method = {{ Alligator_SignalOpenMethod | default(0) }};
    signal_open_filter_method = {{ Alligator_SignalOpenFilterMethod | default(1) }};
    signal_open_level = (float){{ Alligator_SignalOpenLevel | default(0) }};
    signal_open_boost = {{ Alligator_SignalOpenBoostMethod | default(0) }};
    signal_close_method = {{ Alligator_SignalCloseMethod | default(0) }};
    signal_close_level = (float){{ Alligator_SignalCloseLevel | default(0) }};
    price_stop_method = {{ Alligator_PriceStopMethod | default(0) }};
    price_stop_level = (float){{ Alligator_SignalCloseLevel | default(1) }};
    tick_filter_method = {{ Alligator_TickFilterMethod  | default(1) }};
    max_spread = {{ Alligator_MaxSpread | default(0) }};
  }
} stg_alli_{{ Timeframe | lower }};
